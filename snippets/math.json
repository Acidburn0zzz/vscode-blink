{
    // MATH

    // vectors
	"dot": {
        "prefix": "dot",
        "body": "dot(${1:a},${1:b})$0",
        "description": "Dot Product\n\naccepts:int1, int2, int3, int4, float1, float2, float3, float4\nreturns:scalar"
    },
    "vec3 a, vec3 b": {
        "prefix": "cross",
        "body": "cross(${1:a},${1:b})$0",
        "description": "Cross Product\n\nreturns:vec3"
    },
    "length": {
        "prefix": "length",
        "body": "length(${1:a})$0",
        "description": "Returns the length of the vector\n\naccepts:int1, int2, int3, int4, float1, float2, float3, float4\nreturns:scalar"
    },

    "normalize": {
        "prefix": "normalize",
        "body": "normalize(${1:a})$0",
        "description": "Returns the result of dividing vector *a* by its length."
    },

